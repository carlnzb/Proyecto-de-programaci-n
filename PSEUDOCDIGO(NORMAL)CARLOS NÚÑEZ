INICIO {
	MOSTRAR "Introduzca una nota" ;
	LEER 'NOTA' ;
	CONDICION1 = ¿ES (NOTA>=19 AND NOTA<=20)? { SI ES 'SI' ;
	    LEER VARIABLE " A = NOTA "
	    MOSTRAR "NOTA ES A"
	}SI ES 'NO' : {
	CONDICIÓN2 = ¿ES (NOTA>=16 AND NOTA<=18)? { SI ES 'SI' ;
	    LEER VARIABLE " B = NOTA "
	    MOSTRAR "NOTA ES B"
	}SI ES 'NO' : {
	    CONDICIÓN3 = ¿ES (NOTA>=13 AND NOTA<=15)? { SI ES 'SI' ;
	        LEER VARIABLE "C = NOTA "
	        MOSTRAR "NOTA ES C"
	        }SI ES 'NO' : {
	           CONDICIÓN4 = ¿ES (NOTA>=10 AND NOTA<=12)? { SI ES 'SI' ;
	               LEER VARIABLE "D = NOTA "
	               MOSTRAR "NOTA ES D"
	               }SI ES 'NO' : {
	               CONDICIÓN5 = ¿ES (NOTA>=1 AND NOTA<=9)? { SI ES 'SI' ;
	                     LEER VARIABLE "E = NOTA "
	                     MOSTRAR "NOTA ES E"
	                     }SI ES 'NO' : {
	                         MOSTRAR "USTED TIENE UN PROBLEMA"

	                    }

	                    } SALIDA DE CONDICIÓN5
	                } SALIDA DE CONDICIÓN4     
                } SALIDA DE CONDICIÓN3
            } SALIDA DE CONDICIÓN2
        } SALIDA DE CONDICIÓN1
FIN }         